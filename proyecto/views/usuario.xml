<odoo>
  <data>
	
	<record model="ir.ui.view" id="proyecto.usuario_tree">
      <field name="name">Usuarios</field>
      <field name="model">res.partner</field>
      <field name="arch" type="xml">
        <tree >
          <field name="name"/>
		<field name="email"/>
        </tree>
      </field>
    </record>


        <record id="proyecto.usuario_form" model="ir.ui.view">
        <field name="name">Usuarios</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="email"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
	

  </data>
</odoo>